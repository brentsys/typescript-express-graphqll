{
  "name": "typescrip-express-graphql",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy-files": "copyfiles -u 1 src/**/*.graphql src/**/*.gql dist/",
    "generate": "graphql-codegen",
    "build": "rimraf dist && swc ./src -d dist --strip-leading-paths && pnpm copy-files",
    "start:docker": "pnpm build && node --inspect=0.0.0.0 dist/index.js",
    "start": "node dist/index.js",
    "build:image": "gcloud builds submit --tag gcr.io/${GOOGLE_CLOUD_PROJECT}/${APPLICATION_ID}:${APPLICATION_VERSION}",
    "gcloud:deploy": "gcloud run deploy --image=gcr.io/${GOOGLE_CLOUD_PROJECT}/${APPLICATION_ID}:${APPLICATION_VERSION} --platform managed",
    "full:deploy": "pnpm build:image && pnpm gcloud:deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "volta": {
    "node": "18.16.0",
    "pnpm": "8.7.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.18",
    "@tsconfig/node18": "^18.2.4",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.9.1",
    "mocha": "^10.7.3",
    "rimraf": "^6.0.1",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@babel/core": "^7.25.2",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.6",
    "@graphql-tools/schema": "^10.0.6",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "debug": "^4.3.6",
    "express": "^4.19.2",
    "firebase-admin": "^12.4.0",
    "global": "^4.4.0",
